<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ctg.pipeline.config.dao.PipelineDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ctg.pipeline.config.entity.Pipeline">
        <id column="id" property="id" />
        <result column="application" property="application" />
        <result column="name" property="name" />
        <result column="description" property="description" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, application, name, descrption, create_by, create_time, update_by, update_time
    </sql>

    <insert id="insertPipeline" parameterType="com.ctg.pipeline.config.entity.Pipeline" useGeneratedKeys="true" keyProperty="id">
        insert into p_cfg_pipeline
            (id, application, name,description, create_by, create_time, update_by, update_time)
        values (#{id}, #{application},#{name}, #{description}, #{createBy},
          #{createTime}, #{updateBy}, #{updateTime})
    </insert>
</mapper>
